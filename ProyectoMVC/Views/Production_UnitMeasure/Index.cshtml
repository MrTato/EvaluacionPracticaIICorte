@model IEnumerable<ProyectoMVC.ServiceReferenceAW.SELECT_Production_UnitMeasure_Result>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear Medida", "Insert", "Production.UnitMeasure", null)
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ModifiedDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UnitMeasureCode)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ModifiedDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UnitMeasureCode)
        </td>
        <td>
            @Html.ActionLink(
                "Editar",
                "Update",
                "Production_UnitMeasure",
                new
                {
                    ModifiedDate = item.ModifiedDate,
                    Name = item.Name,
                    UnitMeasureCode = item.UnitMeasureCode
                },
                null
            ) |
            @Html.ActionLink(
                "Borrar",
                "Delete",
                "Production_UnitMeasure",
                new
                {
                    ModifiedDate = item.ModifiedDate,
                    Name = item.Name,
                    UnitMeasureCode = item.UnitMeasureCode
                },
                null
            )
        </td>
    </tr>
}

</table>
